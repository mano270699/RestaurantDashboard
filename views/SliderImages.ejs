        <!--Start Model Add New Image-->
        <div class="modal fade" id="AddNewImage">
            <div class="modal-dialog">
                <div class="modal-content">

                    <!-- Modal Header -->
                    <div class="modal-header" >
                        <h5 class="modal-title" ><p class="textstayle">A<span>dd</span> N<span>ew</span> S<span>lider</span></p></h5>
                        <button type="button" class="close" data-dismiss="modal" style="color: #f1f1f1">×</button>
                    </div>

                    <!-- Modal body -->
                    <div class="modal-body">
                        <form id="FormAddNewImage" enctype="multipart/form-data">


                            <div class="form-group">
                                <label for="AddImage_SelectProduct">Product Name</label>
                                <select class="custom-select" id="AddImage_SelectProduct" required>

                                </select>

                            </div>


                            <label for="AddNewImage_SliderImage">Slider Image</label>
                            <div class="custom-file ">
                                <input type="file" class="custom-file-input" id="AddNewImage_SliderImage" accept="image/*" required="">
                                <label class="custom-file-label" for="AddNewImage_SliderImage">Choose file</label>
                            </div>

                            <div class="row justify-content-center">
                                <div class="col-md-5 mt-3">
                                    <input type="submit" class="btn btn-success form-control" style="background-color: #FA640F;border: none;" value="Add">
                                </div>
                                <div class="col-md-5 mt-3">
                                    <input type="button" class="btn btn-secondary btn-md form-control" data-dismiss="modal" value="Close">
                                </div>
                            </div>
                        </form>
                    </div>



                </div>
            </div>
        </div>
        <!--End Model Add New Image-->








        <!--Start Model Remove  SliderImage-->
        <div class="modal fade" id="ActionRemoveSliderImage">
            <div class="modal-dialog ">
                <div class="modal-content">

                    <!-- Modal Header -->
                    <div class="modal-header" >

                            <h5 class="modal-title" ><p class="textstayle"><span><i class="fa fa-trash" aria-hidden="true"></i></span> R<span>emove</span> S<span>lider</span> I<span>mage</span></p></h5>
                             
                        <button type="button" class="close" data-dismiss="modal" style="color: #000000">×</button>
                    </div>

                    <!-- Modal body -->
                    <div class="modal-body">
                        <form id="FormRemoveSliderImage">
                            <div class="form-group">
                                <label for="RemoveSliderImage_ProName ">
                                    <div class="alert alert-danger" role="alert">
                                    <i class="fas fa-exclamation-triangle text-danger"> </i> You will Remove
                                    <span class="text-danger" id="RemoveSliderImage_ProName"></span> from Slider Image.
                                </label>
                                <p class="font-weight-bold">Are you sure to Remove
                                    <span class="text-danger" id="RemoveSliderImage_ProName"></span> from Slider Image?</p>
                                </div>
                                <input id="RemoveSliderImage_SelectProduct" hidden>
                            </div>


                            <div class="row justify-content-center">
                                <div class="col-md-5 mt-3">
                                    <input type="submit" class="btn btn-primary form-control"  value="Yes">
                                </div>
                                <div class="col-md-5 mt-3">
                                    <input type="button" class="btn btn-secondary btn-md form-control"  data-dismiss="modal" value="No">
                                </div>
                            </div>
                        </form>
                    </div>



                </div>
            </div>
        </div>
        <!--End Model Remove SliderImage-->

        
        <!--Start Model   SliderImage-->
        <div class="modal fade SliderImageModel" id="myPreviewImageModal">
                <span class="SliderImageModelClose">&times;</span>
            <div class="modal-dialog ">
                <div class="modal-content">

                    

                    <!-- Modal body -->
                    <div class="modal-body m-0 p-0">
                        <img id="imgPreviewModel" src="" class="img-fluid" />
                    </div>



                </div>
            </div>
        </div>
        <!--End Model  SliderImage-->




